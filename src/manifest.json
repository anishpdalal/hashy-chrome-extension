{
  "manifest_version": 3,
  "name": "Hashy",
  "description": "Search your company's knowledge while working with your most important tools.",
  "version": "0.0.0.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hashy-128.png"
  },
  "icons": {
    "128": "hashy-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "css": [
        "content.styles.css"
      ]
    }
  ],
  "host_permissions": [
    "https://api.nlp-labs.com/*"
  ],
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "hashy-128.png",
        "hashy-34.png"
      ],
      "matches": []
    }
  ]
}